# Cyoda OMS Backend Configuration
# Copy this file to .env and fill in your actual Cyoda credentials

# Cyoda Platform Configuration
CYODA_CLIENT_ID=your-client-id-here
CYODA_CLIENT_SECRET=your-client-secret-here
CYODA_BASE_URL=https://your-cyoda-instance.com
CYODA_GRPC_URL=your-grpc-endpoint:443

# Application Configuration
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=default

# Logging Configuration
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_JAVA_TEMPLATE=DEBUG
